<map proto="1.3.2">
  <name>GS: Mansion</name>
  <version>1.0.0</version>
  <objective>Kill the enemy's squadron!</objective>
  <include src="gs-classes.xml"/>
  <authors>
    <author contribution="Creator of the map and XML.">Timothee38</author>
  </authors>
  <contributors>
    <contributor contribution="Feedback.">salalu</contributor>
    <contributor contribution="Feedback.">Avaleer</contributor>
  </contributors>
  <teams>
    <team color="blue" max="8">Blue Team</team>
    <team color="dark red" max="8">Red Team</team>
  </teams>
  <spawns>
    <spawn team="red" yaw="135">
      <cuboid min="-6,2,-703" max="-4,2,-704"/>
    </spawn>
    <spawn team="blue" yaw="-45">
      <cuboid min="-31,2,-731" max="-33,2,-728"/>
    </spawn>
    <default yaw="90">
      <cuboid min="-17,7,-715" max="-18,7,-717"/>
    </default>
  </spawns>
  <regions>
    <rectangle name="main-area" min="-oo,-oo" max="oo,oo"/>
    <apply block="deny-all">
      <region name="main-area"/>
    </apply>
  </regions>
</map>